<link rel="stylesheet" href="/stylesheets/login.css">


<section class="login-form">

<form action="javascript:void(0);">
  <h1>Log In</h1>
  <div class="form-input-material">
    <label for="username">User Email</label>
    <input type="text" name="useremail" id="useremail" placeholder="Account Email" autocomplete="off" class="form-control-material" required />
  </div>
  <div class="form-input-material">
    <label for="password">Password</label>
    <input type="password" name="password" id="password-login" placeholder="Password" autocomplete="off" class="form-control-material" required />
  </div>
  <button type="submit" class="btn btn-primary btn-ghost">Login</button>
</form>
</section>

<script src="/javascript/login.js"></script>